{
    "name": "chrismou/phergie-irc-plugin-react-php",
    "description": "Phergie plugin for PHP function lookups",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Chris Chrisostomou",
            "email": "dev@mou.me",
            "homepage": "https://mou.me",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "phergie@mou.me.uk",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/chrismou/phergie-irc-plugin-react-php/issues",
        "source": "https://github.com/chrismou/phergie-irc-plugin-react-php"
    },
    "require": {
        "phergie/phergie-irc-bot-react": "~1",
        "phergie/phergie-irc-plugin-react-command": "~1.0",
        "doctrine/dbal": "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "mockery/mockery": "~0.9",
        "vectorface/dunit": "~2",
        "phergie/phergie-irc-plugin-react-autojoin": "1.0.*",
        "phergie/phergie-irc-plugin-react-pong": "~1",
        "phergie/phergie-irc-plugin-react-commandhelp": "~1"

    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-commandhelp": "provides interactive help for commands"
    },
    "autoload": {
        "psr-4": {
            "Chrismou\\Phergie\\Plugin\\Php\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrismou\\Phergie\\Tests\\Plugin\\Php\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
